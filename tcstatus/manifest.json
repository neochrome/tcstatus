{
	"name":"TeamCity Status Notifier",
	"version":"0.2.0",
	"update_url":"https://github.com/neochrome/tcstatus/raw/master/updates.xml",
	"homepage_url":"https://github.com/neochrome/tcstatus",
	"description":"Shows the current status of build configurations on your TeamCity build server.",
	"icons":{
		"32":"images/icon32.png",
		"128":"images/icon128.png"
	},
	"background":"views/background.html",
	"permissions":[
		"http://*/",
		"tabs",
		"notifications"
	],
	"browser_action":{
		"default_icon":"images/icon32.png",
		"default_popup":"views/status.html"
	},
	"options_page":"views/options.html",
  "manifest_version": 2
}
